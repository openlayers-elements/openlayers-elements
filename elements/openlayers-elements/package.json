{
  "name": "@openlayers-elements/maps",
  "version": "0.1.1",
  "description": "Custom elements to declaratively build OpenLayers map",
  "repository": "https://github.com:zazuko/openlayers-elements",
  "author": "Zazuko GmbH",
  "contributors": [
    "tpluscode <tpluscode@users.noreply.github.com>"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "prepare": "yarn build",
    "pretest": "yarn build",
    "remap-coverage": "cat ./coverage/coverage-final.json | yarn remap-istanbul -o ./coverage/lcov.info -t lcovonly",
    "test": "karma start",
    "test:watch": "karma start --auto-watch=true --single-run=false",
    "test:es5": "karma start karma.es5.config.js",
    "test:es5:watch": "karma start karma.es5.config.js --auto-watch=true --single-run=false",
    "test:es5:bs": "karma start karma.es5.bs.config.js"
  },
  "files": [
    "**/*.d.ts",
    "**/*.js"
  ],
  "devDependencies": {
    "@open-wc/testing": "^0.9.19",
    "@open-wc/testing-karma": "^0.4.11",
    "@open-wc/testing-karma-bs": "^0.2.12",
    "@types/mocha": "^5.2.6",
    "@types/ol": "^4.4.2",
    "remap-istanbul": "^0.13.0",
    "standard": "^12.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "resize-observer-polyfill": "^1.5.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
