{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "declaration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "noEmitOnError": false,
    "sourceMap": true,
    "types": [
      "mocha"
    ],
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "noImplicitAny": false
  }
}
